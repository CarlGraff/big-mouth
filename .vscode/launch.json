  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "get-restaurants",
          "program": "${workspaceFolder}/node_modules/.bin/sls",
          "args":[
              "invoke",
              "local",
              "-f",
              "get-restaurants",
              "-d",
              "{}"
          ],
          "env":{
              "restaurants_table": "restaurants"
          }
      },
      {
          "type": "node",
          "request": "launch",
          "name": "search-restaurants",
          "program": "${workspaceFolder}/node_modules/.bin/sls",
          "args":[
              "invoke",
              "local",
              "-f",
              "search-restaurants",
              "-p",
              "examples/search-restaurants.json"
          ],
          "env":{
              "restaurants_table": "restaurants"
          }
      },
      {
        "type": "node",
        "request": "launch",
        "name": "place_order",
        "program": "${workspaceFolder}/node_modules/.bin/sls",
        "args":[
